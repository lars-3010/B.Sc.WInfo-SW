{
    "java.project.sourcePaths": ["src"],
    "java.project.outputPath": "bin",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "javadoc-tools.generateJavadoc.workspaceSourceFolder": "src",
    "javadoc-tools.generateJavadoc.targetFolder": "javadoc",
    "javadoc-tools.generateJavadoc.runMode": "-public",
    "java.docCommentsGenerate": true
}
